<?xml version="1.0" encoding="UTF-8"?>
<web-app
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://JAVA.sun.com/xml/ns/javaee        http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
    version="3.0">
     
    <display-name>MongoDb JSP and SERVLET Example</display-name>
    
    
    
     <servlet>
      <servlet-name>register</servlet-name>
      <servlet-class>com.jcg.mongodb.servlet.Register</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>register</servlet-name>
      <url-pattern>/register</url-pattern>
   </servlet-mapping>
   
   
    <servlet>
      <servlet-name>DashBoard</servlet-name>
      <servlet-class>com.jcg.mongodb.servlet.DashBoard</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>DashBoard</servlet-name>
      <url-pattern>/DashBoard</url-pattern>
   </servlet-mapping>
   
    <servlet>
      <servlet-name>login</servlet-name>
      <servlet-class>com.jcg.mongodb.servlet.Login</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>login</servlet-name>
      <url-pattern>/login</url-pattern>
   </servlet-mapping>
    
    
     <servlet>
      <servlet-name>logout</servlet-name>
      <servlet-class>com.jcg.mongodb.servlet.Logout</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>logout</servlet-name>
      <url-pattern>/logout</url-pattern>
   </servlet-mapping>
    
    
    <servlet>
      <servlet-name>uniqueID</servlet-name>
      <servlet-class>com.jcg.mongodb.servlet.UniqueID</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>uniqueID</servlet-name>
      <url-pattern>/uniqueID</url-pattern>
   </servlet-mapping>
    
    
   <servlet>
      <servlet-name>Search</servlet-name>
      <servlet-class>com.jcg.mongodb.servlet.Search</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>Search</servlet-name>
      <url-pattern>/Search</url-pattern>
   </servlet-mapping>
   
     
   <servlet>
      <servlet-name>Test</servlet-name>
      <servlet-class>com.jcg.mongodb.servlet.Test</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>Test</servlet-name>
      <url-pattern>/Test</url-pattern>
   </servlet-mapping>
   
   
   
     
     <servlet>
      <servlet-name>YourQuiz</servlet-name>
      <servlet-class>com.jcg.mongodb.servlet.YourQuiz</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>YourQuiz</servlet-name>
      <url-pattern>/YourQuiz</url-pattern>
   </servlet-mapping>
    

       
    <servlet>
      <servlet-name>Preview</servlet-name>
      <servlet-class>com.jcg.mongodb.servlet.Preview</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>Preview</servlet-name>
      <url-pattern>/Preview</url-pattern>
   </servlet-mapping>
     
     

 <servlet>
      <servlet-name>submit</servlet-name>
      <servlet-class>com.jcg.mongodb.servlet.Submit</servlet-class>
   </servlet>
   <servlet-mapping>
      <servlet-name>submit</servlet-name>
      <url-pattern>/submit</url-pattern>
   </servlet-mapping>

   
  
    <welcome-file-list>
        <welcome-file>dashBoard.html</welcome-file>
    </welcome-file-list>
</web-app>